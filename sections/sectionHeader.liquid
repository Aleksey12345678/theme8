</div>
<style>
  .sectionHeader__logo{
    padding:{{section.settings.padding}}px 0;
  }
</style>

<script>
  document.querySelector('.sectionHeader-hb').addEventListener('click',()=> {
    document.querySelector('.sectionHeader').classList.toggle('nav-active');
  } );
   document.querySelector('.sectionHeader__mobileNav_header__closeImg').addEventListener('click',()=> {
    document.querySelector('.sectionHeader').classList.remove('nav-active');
  } );
</script>
{% schema %}
{
  "name": "sectionHeader",
  "tag":"header",
  "class": "header",
  "settings": [

     {
          "type":"image_picker",
          "id":"sectionHeader_logo",
          "label":"Logo"
        },
     {
          "type":"image_picker",
          "id":"sectionHeader_mobilLogo",
          "label":"Mobile Logo"
        },
     {
        "type": "range",
        "id":"padding",
        "label": "Header height",
        "default":1,
        "min":0,
        "max": 100,
        "step": 1
      },


    {
      "type": "link_list",
      "id": "navigation",
      "label": "Navigation menu",
      "default":"main-menu"
    },
      {
          "type":"inline_richtext",
          "id":"buttonText",
          "default":" CONTACT US",
          "label":"Button text"
      }
  ],
   "blocks":[
 ],
    "presets":[
  {
    "name":"sectionHeader",
    "blocks": [

      ]
  }
]
}
{% endschema %}
